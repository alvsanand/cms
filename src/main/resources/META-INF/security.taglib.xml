<?xml version="1.0"?>
<facelet-taglib
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
        version="2.0">


    <namespace>http://alvsanand.appspot.com/security/facelets/tags</namespace>

    <function>
        <function-name>ifAnyGranted</function-name>
        <function-class>es.alvsanand.webpage.security.taglibs.facelets.SecurityELLibrary</function-class>
        <function-signature>boolean ifAnyGranted(java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>ifAllGranted</function-name>
        <function-class>es.alvsanand.webpage.security.taglibs.facelets.SecurityELLibrary</function-class>
        <function-signature>boolean ifAllGranted(java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>ifNotGranted</function-name>
        <function-class>es.alvsanand.webpage.security.taglibs.facelets.SecurityELLibrary</function-class>
        <function-signature>boolean ifNotGranted(java.lang.String)</function-signature>
    </function>


    <tag>
        <tag-name>ifAnyGranted</tag-name>
        <component>
            <component-type>es.alvsanand.webpage.security.taglibs.facelets.IfAnyGrantedTag</component-type>
            <handler-class>es.alvsanand.webpage.security.taglibs.facelets.IfAnyGrantedTag</handler-class>
        </component>
    </tag>


    <tag>
        <tag-name>ifAllGranted</tag-name>
        <component>
            <component-type>es.alvsanand.webpage.security.taglibs.facelets.IfAllGrantedTag</component-type>
            <handler-class>es.alvsanand.webpage.security.taglibs.facelets.IfAllGrantedTag</handler-class>
        </component>
    </tag>


    <tag>
        <tag-name>ifNotGranted</tag-name>
        <component>
            <component-type>es.alvsanand.webpage.security.taglibs.facelets.IfNotGrantedTag</component-type>
            <handler-class>es.alvsanand.webpage.security.taglibs.facelets.IfNotGrantedTag</handler-class>
        </component>
    </tag>

</facelet-taglib>