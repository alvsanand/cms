        <function-class>es.alvsanand.webpage.common.taglibs.facelets.StringELLibrary</function-class>
        <function-signature>java.lang.String concat(java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>replace</function-name>
        <function-class>es.alvsanand.webpage.common.taglibs.facelets.StringELLibrary</function-class>
        <function-signature>java.lang.String replace(java.lang.String, java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>getMessage</function-name>
        <function-class>es.alvsanand.webpage.common.taglibs.facelets.StringELLibrary</function-class>
        <function-signature>java.lang.String getMessage(java.lang.String, java.lang.String)</function-signature>
    </function>
